{
  "name": "somov/yii2-model-files",
  "description": "Yii2 model files",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "file",
    "model"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Somov",
      "email": "somov.nn@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": ">=2.0.14"
  },
  "autoload": {
    "psr-4": {
      "somov\\mfiles\\": "src"
    }
  },
  "archive": {
    "exclude": [
      "/.*",
      "/tests",
      "/vendor",
      "/bin"
    ]
  }
}
